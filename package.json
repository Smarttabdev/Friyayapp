  "scripts": {
    "start": "DEBUG=express:* node ./server",
    "run": "webpack-dev-server --progress --colors --inline --port 5000 --content-base public/",
    "dev": "webpack-dev-server --progress --colors --port 5000 --disable-host-check --config webpack.development.config.js",
    "dev:debug": "DEBUG=1 npm run dev",
    "build": "webpack --progress --watch",
    "build:analyse": "analyse=true webpack --progress --watch",
    "build:sw": "webpack --progress --watch --config webpack.sw.config.js",
    "relay": "relay-compiler --src src/ --schema schema.graphql.json --extensions=js",
    "watch": "npm-watch",
    "test": "jest",
    "lint": "eslint --ext js --ext jsx src || exit 0",
    "lint:errors": "eslint --ext js --ext jsx --quiet src || exit 0",
    "lint:fix": "eslint --fix --ext js --ext jsx src || exit 0",
    "validate": "npm ls",
    "prettify-code": "pretty-quick --staged",
    "build:desktop:dev": "nativefier -n 'Friyay Staging' --internal-urls '(.*?)(accounts\\.google\\.com|slack\\.com|friyayapp\\.localhost)(.*?)' --counter --min-width 1024px --min-height 768px --single-instance 'http://friyayapp.localhost:5000'",
    "build:desktop:mac": "nativefier -n 'Friyay' -p mac --internal-urls '(.*?)(accounts\\.google\\.com|slack\\.com|friyayapp\\.io)(.*?)' --counter --bounce --min-width 1024px --min-height 768px -c --single-instance --disable-dev-tools 'https://friyayapp.io'",
    "build:desktop:windows": "nativefier -n 'Friyay' -p windows --internal-urls '(.*?)(accounts\\.google\\.com|slack\\.com|friyayapp\\.io)(.*?)' --counter --min-width 1024px --min-height 768px -c --single-instance --disable-dev-tools 'https://friyayapp.io'",
    "build:desktop:linux": "nativefier -n 'Friyay' -p linux --internal-urls '(.*?)(accounts\\.google\\.com|slack\\.com|friyayapp\\.io)(.*?)' --counter --min-width 1024px --min-height 768px -c --single-instance --disable-dev-tools 'https://friyayapp.io'"
  },
  "watch": {
    "relay": {
      "patterns": [
        "src",
        "schema.graphql",
        "schema.graphql.json"
      ],
      "extensions": [
        "js"
      ],
      "ignore": [
        "node_modules"
      ]
    }
  },
  "engines": {
    "node": "^10.20.1",
    "npm": "^6.14.4"
  },
  "jest": {
    "setupFiles": [
      "jest-localstorage-mock",
      "./test/setup.js"
    ],
    "rootDir": "./src"
  },
  "author": "TipHive Inc.",
  "pre-commit": [
    "test",
    "prettify-code"
  ]
}
